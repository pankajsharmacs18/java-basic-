package copy;

import java.util.Scanner;

public class Copy {
    Scanner input = new Scanner(System.in);

    public void ArrayIpnut( int range) {


        System.out.println("enter roll no of student");
        int roll[] = new int[range];
        for (int i = 0; i < range; i++) {
            roll[i] = input.nextInt();

        }
        System.out.println(" array is copied ");
        int copyArray[] = new int[range];
        for (int i = 0; i < range; i++) {
            copyArray[i] = roll[i];


        System.out.println(" copies arrasy is ");

            for (i = 0; i < range; i++) {
                System.out.println(copyArray[i]);

            }

        }
    }

    public static void main(String args[]){
            Scanner RangeInput = new Scanner(System.in);
    int range =RangeInput.nextInt();
    Copy obj = new Copy();
    obj.ArrayIpnut(range);


    }
}
