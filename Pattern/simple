package pattern1;
import java.util.Scanner;

public class Pattern {
    public static void main(String args[])
    {
        Scanner input = new Scanner(System.in);
        int range=0;
        System.out.println("Enter count");
        range = input.nextInt();
        for (int i = 0; i <=range ; i++) {
            for (int j = 0; j <=i ; j++) {
                System.out.print("#");

            }
            System.out.println();
        }


    }
}
